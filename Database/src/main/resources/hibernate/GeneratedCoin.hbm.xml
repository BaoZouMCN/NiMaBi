<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.baozou.NiMaBi.Database.Model.GeneratedCoin" table="generated_coin">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="coin_uuid" type="string">
            <column name="coin_uuid" length="32" not-null="true" />
        </property>
        <property name="coin_type" type="java.lang.Integer">
            <column name="coin_type" length="10" not-null="true" />
        </property>
        <property name="current_event_id" type="java.lang.Integer">
            <column name="current_event_id" length="10" not-null="true" default="1" />
        </property>
    </class>
</hibernate-mapping>