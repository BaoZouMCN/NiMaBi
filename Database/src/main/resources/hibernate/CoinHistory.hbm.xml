<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.baozou.NiMaBi.Database.Model.CoinHistory" table="coin_history">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="coin_uuid" type="string">
            <column name="coin_uuid" length="32" not-null="true" unique="true" />
        </property>
        <property name="event_id" type="java.lang.Integer">
            <column name="event_id" length="10" not-null="true" />
        </property>
        <property name="time" type="java.sql.Timestamp.">
            <column name="time" not-null="false" />
        </property>
    </class>
</hibernate-mapping>