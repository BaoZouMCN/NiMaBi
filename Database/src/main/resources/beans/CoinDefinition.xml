<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <!-- CoinDefinition business object -->
    <bean id="coinBO" class="com.baozou.NiMaBi.Database.BusinessObject.CoinDefinitionBOImpl" >
        <property name="coinDefinitionDAO" ref="coinDefinitionDAO" />
    </bean>
    <!-- CoinDefinition Data Access Object -->
    <bean id="coinDefinitionDAO" class="com.baozou.NiMaBi.Database.DAO.CoinDefinitionDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

</beans>