<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <!-- GeneratedCoin business object -->
    <bean id="generatedCoinBO" class="com.baozou.NiMaBi.Database.BusinessObject.GeneratedCoinBOImpl" >
        <property name="generatedCoinDAO" ref="generatedCoinDAO" />
    </bean>
    <!-- GeneratedCoin Data Access Object -->
    <bean id="generatedCoinDAO" class="com.baozou.NiMaBi.Database.DAO.GeneratedCoinDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

</beans>